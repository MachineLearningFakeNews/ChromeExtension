{
  "manifest_version": 2,

  "name": "Fake News Gauge",
  "description": "This extension displays a credibility percentage of a new source currently opened in the tab.",
  "version": "1.0",
  "icons": {
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Check Source"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "https://ajax.googleapis.com/"
  ]
}